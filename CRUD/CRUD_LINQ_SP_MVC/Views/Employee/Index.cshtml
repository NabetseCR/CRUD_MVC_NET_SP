@model IEnumerable<CRUD_LINQ_SP_MVC.SP_EMPResult>

@{
    ViewBag.Title = "Index";
}

<h2>Employees</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EMID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMNAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMAIL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SALARY)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EMID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMNAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMAIL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SALARY)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EMID }) |
            @Html.ActionLink("Details", "Details", new { id = item.EMID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.EMID })
        </td>
    </tr>
}

</table>
